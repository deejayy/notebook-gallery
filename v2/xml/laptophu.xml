	<prepareProduct><![CDATA[
		$spec = $product['.technikai-jellemzok-tablazat li'];
		foreach ($spec as $specLine) {
			pq($specLine)->html('<div class=\'shop_parameter\'>' .
				implode('</div><div class=\'shop_ertek\'>', preg_split('/:/', pq($specLine)->text())) .
				'</div>'
			);
		}
	]]></prepareProduct>
